{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "ui/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "ui/$1",
            "continue": true
        },
        {
            "src": "/",
            "dest": "ui/index.html"
        }
    ]
}